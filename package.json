{
  "name": "ice-cream",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:stats": "ng build  --stats-json",
    "analyse": "webpack-bundle-analyzer dist/ice-cream/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write",
    "format:check": "prettier --check",
    "test:coverage": "jest --coverage",
    "e2e": "ng e2e",
    "build:doc": "compodoc -p tsconfig.json -d dist/apps/documentation/compodoc --theme vagrant",
    "serve:doc": "compodoc -p tsconfig.json -d dist/apps/documentation/compodoc --theme vagrant -s",
    "serve:json": "json-server server/db.json",
    "serve:web": "ng serve --host 0.0.0.0 --open",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "ng-zorro-antd": "11.3.0",
    "rxjs": "6.6.7",
    "subsink": "1.0.2",
    "tslib": "2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "12.0.0",
    "@angular/cli": "12.0.0",
    "@angular/compiler-cli": "12.0.0",
    "@nrwl/cypress": "11.6.1",
    "@nrwl/jest": "11.6.1",
    "@types/jest": "26.0.22",
    "@types/node": "12.20.7",
    "angular-http-server": "1.10.0",
    "codelyzer": "6.0.1",
    "cypress": "6.8.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "json-server": "0.16.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "ts-node": "8.3.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4",
    "webpack-bundle-analyzer": "4.4.0"
  }
}
